	journal = {Nature},
	author = {Gao, Yang and Yang, Xiaofei and Chen, Hao and Tan, Xinjiang and Yang, Zhaoqing and Deng, Lian and Wang, Baonan and Kong, Shuang and Li, Songyang and Cui, Yuhang and Lei, Chang and Wang, Yimin and Pan, Yuwen and Ma, Sen and Sun, Hao and Zhao, Xiaohan and Shi, Yingbing and Yang, Ziyi and Wu, Dongdong and Wu, Shaoyuan and Zhao, Xingming and Shi, Binyin and Jin, Li and Hu, Zhibin and Lu, Yan and Chu, Jiayou and Ye, Kai and Xu, Shuhua},
